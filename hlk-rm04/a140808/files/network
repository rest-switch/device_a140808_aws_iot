
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option ifname 'eth0'
	option proto 'dhcp'

config interface 'lan6'
	option ifname 'eth0'
	option proto 'dhcpv6'

config interface 'wan'
	option ifname 'wlan0'
	option proto 'dhcp'

config interface 'wan6'
	option ifname 'wlan0'
	option proto 'dhcpv6'

config switch
	option name 'switch0'
	option reset '1'
	option enable_vlan '0'
